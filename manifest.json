{
  "name": "Resumme Keeper",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Extension to store your resumes in browser",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}


